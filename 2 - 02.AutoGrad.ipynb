   "metadata": {},
   "outputs": [],
   "source": [
    "w = torch.tensor(2.0, requires_grad = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "c776143d-2255-46b7-84c9-4d92a0baaa1d",
   "metadata": {},
   "outputs": [],
   "source": [
    "y = w**2\n",
    "z = 2*y + 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "7f5a43fc-2d36-418e-9247-44b50272a201",
   "metadata": {},
   "outputs": [],
   "source": [
    "z.backward()"
   ]
  },
